        "sass-loader": "^8.0.0",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "laravel-mix-polyfill": "^2.0.0",
        "qs": "^6.9.4",
        "v-viewer": "^1.5.1",
        "vue-lazyload": "^1.3.3",
